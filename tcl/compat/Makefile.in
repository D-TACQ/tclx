SHELL = /bin/sh

#------------------------------------------------------------------------------
# Autoconfig defines that can be overridden in Config.mk

RANLIB  = @RANLIB@
LIBOBJS = @LIBOBJS@

#------------------------------------------------------------------------------
# Include user-editable defines.

@MAKEINCLUDE@ @MAKEQUOTE@../Config.mk@MAKEQUOTE@

#------------------------------------------------------------------------------

LIBTCLX.A = ../tclmaster/lib/libtclx.a

CFLAGS = ${OPTIMIZE_FLAG} ${XCFLAGS} -I../${TCL_UCB_DIR}

#------------------------------------------------------------------------------

all: made.tmp

made.tmp: ${LIBOBJS}
	if [ "${LIBOBJS}" != "" ] ; then \
	    ${AR} cvr ${LIBTCLX.A} ${LIBOBJS} ;\
	    ${RANLIB} ${LIBTCLX.A} ;\
	else \
	    exit 0 ;\
	fi
	touch made.tmp

#------------------------------------------------------------------------------

clean:
	-rm -f made.tmp
	-rm -f *.o

#------------------------------------------------------------------------------
# Restore to the distributed state.

distclean: clean
	rm -f Makefile
