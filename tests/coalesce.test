package require Tclx
if {[cequal [info procs Test] {}]} {
    source [file join [file dirname [info script]] testlib.tcl]
}

Test coalesce-1.1 {coalesce tests} {
    coalesce a b c d default
} 0 {default}

Test coalesce-1.2 {coalesce tests} {
	coalesce
} 1 {wrong # args: coalesce var ?var...? defaultValue}

Test coalesce-1.3 {coalesce tests} {
	coalesce a
} 1 {wrong # args: coalesce var ?var...? defaultValue}

Test coalesce-1.4 {coalesce tests} {
	set a pluto
    coalesce a b c d default
} 0 {pluto}

Test coalesce-1.5 {coalesce tests} {
	unset a
	set b goofy
    coalesce a b c d default
} 0 {goofy}

Test coalesce-1.6 {coalesce tests} {
	set a pluto
	set b goofy
    coalesce a b c d default
} 0 {pluto}

Test coalesce-1.7 {coalesce tests} {
	unset a b
	set c minnie
    coalesce a b c d default
} 0 {minnie}

Test coalesce-1.8 {coalesce tests} {
	unset c
	set d mickey
    coalesce a b c d default
} 0 {mickey}

Test coalesce-1.9 {coalesce tests} {
	unset d
	set default no-way
    coalesce a b c d default
} 0 {default}

# cleanup
::tcltest::cleanupTests
return
